{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "curly": [true, "as-needed"],
    "interface-name": [true, "never-prefix"],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^export {(.*?)}"
      }
    ],
    "member-ordering": [true, { "order": "instance-sandwich" }],
    "no-debugger": true,
    "one-line": true
  }
}
